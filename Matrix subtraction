#include<iostream>
int main()
{
	int r,c,a[100][100],b[100][100],diff[100][100],i,j;
	cout<<"enter the number of rows:";
	cin>>r;
	cout<<"enter th number of columns:";
	cin>>c;
	cout<<"enter the first matrix";
	for(i=0;i<r;i++)
	 for(j=0;j<c;j++)
	 {
	 	cin>>a[i][j];
	 }
	 cout<<"enter the second matrix";
	 for(i=0;i<r;i++)
	  for(j=0;j<c;j++)
	  {
	  	cin>>b[i][j];
	  }
	  for(i=0;i<r;i++)
	   for(j=0;j<c;j++)
	   {
	   	diff[i][j]=a[i][j]-b[i][j];
	   }
	   for(i=0;i<r;i++)
	    for(j=0;j<c;j++)
	    {
	    	cout<<diff[i][j]<<" ";
	    	if(j==c -1)
	    	cout<< endl;
		}
		return 0;
	
}
